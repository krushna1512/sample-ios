lane :build_only do
  match(
    type: "deployment",
    app_identifier: "com.yapsody.SampleApp"
    # remove readonly if you want match to create/update profiles
    # readonly: true
  )

  gym(
    project: "SampleApp.xcodeproj",
    scheme: "SampleApp",
    clean: true,
    export_method: "deployment",
    output_directory: "./build",
    output_name: "SampleApp.ipa"
  )
end

